    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "emotion-normalize": "^11.0.1",
    "eslint": "^8.44.0",
    "jotai": "^2.0.4",
    "next": "13.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "turbo": "^1.10.7",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/golang-wasm-exec": "^1.15.0",
    "@types/node": "18.16.0",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11"
  }
}
